{
	"name": "spameri/elastic",
    "type": "library",
    "description": "ElasticSearch for Nette Framework",
    "keywords": [
        "nette",
        "spameri",
        "elastic",
        "elasticsearch"
    ],
    "homepage": "http://github.com/spameri",
    "license": [
        "BSD-3-Clause",
        "GPL-2.0",
        "GPL-3.0"
    ],
    "support": {
        "issues": "https://github.com/spameri/elastic/issues"
    },
	"authors": [
		{
			"name": "VBoss",
			"email": "vcevela@gmail.com"
		}
